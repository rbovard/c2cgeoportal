{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["es2020", "dom"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "noEmit": true,
    "checkJs": false,
    "types": ["cypress"],
    linterOptions: {
      exclude: ['**/ngeo/**'],
    }
  },
  "include": ["{{package}}_geoportal/static-ngeo/js/**/*.js"]
}
